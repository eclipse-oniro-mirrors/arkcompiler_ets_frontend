class C{}

function process1(con: (this:C)=>void){
    let c = new C();
    c.con();
}

function process2(this:C):string{
    return "process2(this:C)"
}

function process2(a:int, b:int):string{
    return "process2(a:int, b:int)"
}

function process2(a:int):string{
    return "process2(a:int)"
}

function main(){
    process1((this:C):void => {
        assertEQ(this.process2() , "process2(this:C)")
        assertEQ(process2() , "process2(this:C)")
        assertEQ(process2(10) , "process2(a:int)")
        assertEQ(process2(10,20) , "process2(a:int, b:int)")
    })

    process1(){
        assertEQ(this.process2() , "process2(this:C)")
        assertEQ(process2() , "process2(this:C)")
        assertEQ(process2(10) , "process2(a:int)")
        assertEQ(process2(10,20) , "process2(a:int, b:int)")
    }
}