class C{
    foo():string{
        return "c.foo()"
    }
}

function process1(con: (this:C)=>void){
    let c = new C();
    c.con();
}

function process2(this:C):string{
    return "process2(this:C)"
}

function process3():string{
    return "process3()"
}

function main(){
    process1((this:C):void => {
        assertEQ(this.foo() , "c.foo()")
        assertEQ(foo() , "c.foo()")
        assertEQ(this.process2() , "process2(this:C)")
        assertEQ(process2() , "process2(this:C)")
        assertEQ(process3() , "process3()")
    })

    process1(){
        assertEQ(this.foo() , "c.foo()")
        assertEQ(foo() , "c.foo()")
        assertEQ(this.process2() , "process2(this:C)")
        assertEQ(process2() , "process2(this:C)")
        assertEQ(process3() , "process3()")
    }
}
