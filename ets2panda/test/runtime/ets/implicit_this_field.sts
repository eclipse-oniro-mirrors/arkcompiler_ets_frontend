class C{
    field:string = "C.field";
    field2:string = "C.field2";
}

function process1(con: (this:C)=>void){
    let c = new C();
    c.con();
}

let field3:string = "global.field";

function process2(a:string, b:string){
    assertEQ(a , b)
}

function main(){
    process1((this:C):void => {
        let field:string = "function.field";

        assertEQ(this.field , "C.field");
        assertEQ(field , "function.field");

        let a = this.field;
        let b = field;

        assertEQ(a , "C.field")
        assertEQ(b , "function.field");

        process2(this.field, "C.field")
        process2(field,"function.field")
    })

    process1((this:C):void => {
        assertEQ(this.field2 , "C.field2");
        assertEQ(field2 , "C.field2");

        let a = this.field2;
        let b = field2;

        assertEQ(a , "C.field2");
        assertEQ(b , "C.field2");

        process2(this.field2,"C.field2")
        process2(field2,"C.field2")
    })

    process1((this:C):void => {
        assertEQ(field3 , "global.field");

        let b = field3;
        assertEQ(b , "global.field");

        process2(field3,"global.field")
    })

    process1(){
        let field:string = "function.field";

        assertEQ(this.field , "C.field");
        assertEQ(field , "function.field");

        let a = this.field;
        let b = field;

        assertEQ(a , "C.field")
        assertEQ(b , "function.field");

        process2(this.field, "C.field")
        process2(field,"function.field")
    }

    process1(){
        assertEQ(this.field2 , "C.field2");
        assertEQ(field2 , "C.field2");

        let a = this.field2;
        let b = field2;

        assertEQ(a , "C.field2");
        assertEQ(b , "C.field2");

        process2(this.field2,"C.field2")
        process2(field2,"C.field2")
    }

    process1(){
        assertEQ(field3 , "global.field");

        let b = field3;
        assertEQ(b , "global.field");

        process2(field3,"global.field")
    }
}
