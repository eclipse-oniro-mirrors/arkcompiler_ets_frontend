import { asyncFunc1, asyncFunc2 } from "./exportAsync_test2.ets"
async function bar1() {
    const result = await asyncFunc1();
    return result;
}

async function bar2() {
    const result = await asyncFunc2(1);
    return result;
}

function main() {
    waitForCompletion(async () => {
        arktest.assertEQ(await bar1(), 1);
        arktest.assertEQ(await bar2(), 1);
    })
}
