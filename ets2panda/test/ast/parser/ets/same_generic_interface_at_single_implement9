interface IBase<T> {
  method(): string;
}

interface IBase1 extends IBase<IBase2> {
  method(): string;
}

interface IBase2 extends IBase<IBase1> {
  method(): string;
}

class D implements IBase1, IBase2 {
  method(): string {
    return "Derived method";
  }
}
